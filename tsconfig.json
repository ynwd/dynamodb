{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "es2015.promise"
      ],
      "noImplicitAny": true,
      "strictNullChecks": true,
      "allowJs": true,
      "noEmit": true,
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   }
}
